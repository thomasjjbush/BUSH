{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Simple reusable React boilerplate.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --env.NODE_ENV=production --mode production",
    "clean": "rm -rf node_modules",
    "cypress:open": "./node_modules/.bin/cypress open",
    "functional": "./node_modules/.bin/cypress run --spec 'cypress/integration/home.functional.spec.ts,cypress/integration/project.functional.spec.ts'",
    "lint": "eslint",
    "start": "webpack-dev-server --env.NODE_ENV=development --open --compress --port 3000",
    "test": "jest",
    "visual-regression": "./node_modules/.bin/cypress run --spec 'cypress/integration/home.visual.spec.ts,cypress/integration/project.visual.spec.ts'"
  },
  "author": "Thomas Bush",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-regenerator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^24.9.1",
    "@types/webpack-env": "^1.15.3",
    "axe-core": "^4.1.1",
    "babel-loader": "^8.2.1",
    "compression-webpack-plugin": "^7.0.0",
    "cypress": "^6.1.0",
    "cypress-axe": "^0.12.0",
    "cypress-image-snapshot": "^4.0.0",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^25.5.4",
    "react-test-renderer": "^16.14.0",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.7",
    "webpack": "^5.10.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": []
  },
  "dependencies": {
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.10",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux": "^3.6.0",
    "@types/redux-batched-actions": "^0.1.5",
    "@types/redux-mock-store": "^1.0.2",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "install": "^0.13.0",
    "npm": "^6.14.9",
    "prettier": "^1.19.1",
    "react": "~16.12.0",
    "react-dom": "~16.12.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.1"
  }
}
